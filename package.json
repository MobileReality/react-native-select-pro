{
  "name": "react-native-select-pro-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "engines": {
    "node": ">=16.15.1",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev-start": "pnpm --filter '@monorepo/expo' run dev-start",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test --filter '@mobile-reality/react-native-select-pro'",
    "e2e:metro": "turbo run detox-metro-start --filter '@monorepo/e2e'",
    "e2e:ios": "turbo run detox-ios-test --filter '@monorepo/e2e'",
    "e2e:android": "turbo run detox-android-test --filter '@monorepo/e2e'",
    "tsc": "turbo run tsc",
    "docs": "yarn --cwd website start",
    "postinstall": "husky install",
    "clean": "turbo run clean && rm -Rf node_modules"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.5",
    "@changesets/cli": "^2.23.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@mobile-reality/eslint-config": "^0.8.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^10.1.1",
    "@types/jest": "^28.1.4",
    "@types/react-native": "^0.68.1",
    "@types/react-test-renderer": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "~5.31.0",
    "@typescript-eslint/parser": "~5.31.0",
    "commitlint": "^17.0.1",
    "eslint": "~8.20.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-test-renderer": "^17.0.2",
    "reassure": "^0.1.1",
    "ts-jest": "^28.0.5",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'pnpm tsc'",
      "eslint --fix"
    ]
  }
}
