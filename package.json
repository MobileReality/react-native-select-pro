{
  "name": "react-native-select-pro-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "engines": {
    "node": ">=16.15.1",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "pnpm --filter '@monorepo/expo' run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test --filter '@mobile-reality/react-native-select-pro'",
    "e2e:metro": "turbo run detox-metro-start --filter '@monorepo/e2e'",
    "e2e:ios": "turbo run detox-ios-test --filter '@monorepo/e2e'",
    "e2e:android": "turbo run detox-android-test --filter '@monorepo/e2e'",
    "tsc": "turbo run tsc",
    "docs": "yarn --cwd website start",
    "postinstall": "husky install",
    "clean": "turbo run clean && rm -Rf node_modules"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.7",
    "@changesets/cli": "2.25.0",
    "@commitlint/config-conventional": "17.1.0",
    "@mobile-reality/eslint-config": "0.8.0",
    "@testing-library/jest-native": "5.0.0",
    "@testing-library/react-native": "11.2.0",
    "@types/jest": "29.1.2",
    "@types/react-native": "0.69.13",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "commitlint": "17.1.2",
    "eslint": "8.20.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "8.0.1",
    "jest": "29.2.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "react": "18.0.0",
    "react-native": "0.69.6",
    "react-test-renderer": "18.0.0",
    "reassure": "0.6.0",
    "ts-jest": "29.0.3",
    "turbo": "1.5.6",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'pnpm tsc'",
      "eslint --fix"
    ]
  }
}
