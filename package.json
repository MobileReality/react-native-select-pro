{
  "name": "@mobile-reality/react-native-select-pro",
  "version": "1.7.0",
  "description": "React Native dropdown (select) component developed by Mobile Reality",
  "engines": {
    "node": ">=12"
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "bob build",
    "release": "release-it",
    "release:beta": "release-it --preRelease=beta --no-git.requireUpstream",
    "tsc": "tsc --noEmit",
    "lint": "eslint ./src ./example ./website --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "expo-start": "yarn --cwd example start",
    "expo-android": "yarn --cwd example android",
    "expo-ios": "yarn --cwd example ios",
    "docs": "yarn --cwd website start",
    "bootstrap": "yarn --cwd example"
  },
  "keywords": [
    "react-native",
    "react",
    "ios",
    "android",
    "select",
    "picker",
    "dropdown",
    "menu",
    "react-native-component"
  ],
  "repository": {
    "type": "git",
    "directory": "https://github.com/MobileReality/react-native-select-pro.git"
  },
  "author": "Mobile Reality <biuro@mobilereality.pl> (https://mobilereality.pl)",
  "contributors": [
    {
      "name": "@irekrog",
      "email": "irekfrontend@gmail.com",
      "url": "https://github.com/irekrog"
    },
    {
      "name": "Marcin Sadowski",
      "url": "https://github.com/gitsad"
    },
    {
      "name": "Agata Kosior",
      "url": "https://github.com/koko57"
    },
    {
      "name": "Mateusz Tkacz",
      "url": "https://github.com/CatchMe2"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MobileReality/react-native-select-pro/issues"
  },
  "homepage": "https://github.com/MobileReality/react-native-select-pro/#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@gorhom/portal": "1.0.12"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@mobile-reality/eslint-config": "^0.1.4",
    "@release-it/conventional-changelog": "^4.2.2",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@types/react-native": "0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "commitlint": "^16.2.1",
    "eslint": "^8.11.0",
    "husky": "4.3.8",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "pod-install": "^0.1.32",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "react-native": "0.67.3",
    "react-native-builder-bob": "^0.18.2",
    "react-test-renderer": "^17.0.2",
    "release-it": "^14.12.5",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": "^16.13 || ^17",
    "react-native": ">=0.63"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn tsc",
      "pre-push": "jest"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint \"**/*.{js,ts,jsx,tsx}\" --fix"
    ]
  }
}
