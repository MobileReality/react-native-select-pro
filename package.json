{
  "name": "react-native-select-pro",
  "version": "0.1.0",
  "description": "React Native dropdown library",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "../react-native-select-pro/src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-select-pro.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/MobileReality/react-native-select-pro",
  "author": "MobileReality <biuro@mobilereality.pl> (https://mobilereality.pl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MobileReality/react-native-select-pro/issues"
  },
  "homepage": "https://github.com/MobileReality/react-native-select-pro/#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@react-native-community/eslint-config": "^3.0.0",
    "@release-it/conventional-changelog": "^3.0.1",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-native": "0.64.10",
    "commitlint": "^12.1.4",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "pod-install": "^0.1.23",
    "prettier": "^2.3.2",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-builder-bob": "^0.18.1",
    "release-it": "^14.10.0",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript",
      "pre-push": "jest"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint \"**/*.{js,ts,jsx,tsx}\" --fix",
      "prettier --write"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@types/react-test-renderer": "^17.0.1",
    "lint-staged": "^11.0.0",
    "react-test-renderer": "^17.0.2"
  }
}
