{
  "name": "@mobile-reality/react-native-select-pro",
  "version": "1.2.0",
  "description": "React Native select / dropdown customizable component",
  "engines": {
    "node": ">=12"
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-select-pro.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "tsc": "tsc --noEmit",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "build": "bob build",
    "release": "release-it",
    "bootstrap": "yarn example && yarn && yarn pods",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "expo-android": "cd example && yarn android",
    "expo-ios": "cd example && yarn ios",
    "docs": "cd website && yarn start",
    "typedoc": "typedoc"
  },
  "keywords": [
    "react-native",
    "react",
    "ios",
    "android",
    "select",
    "picker",
    "dropdown",
    "menu",
    "react-native-component"
  ],
  "repository": "https://github.com/MobileReality/react-native-select-pro",
  "author": "MobileReality <biuro@mobilereality.pl> (https://mobilereality.pl)",
  "contributors": [
    {
      "name": "@irekrog",
      "url": "https://github.com/irekrog"
    },
    {
      "name": "Marcin Sadowski",
      "url": "https://github.com/gitsad"
    },
    {
      "name": "Mateusz Tkacz",
      "url": "https://github.com/CatchMe2"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MobileReality/react-native-select-pro/issues"
  },
  "homepage": "https://github.com/MobileReality/react-native-select-pro/#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@gorhom/portal": "1.0.11",
    "react-native-gesture-handler": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@release-it/conventional-changelog": "^3.3.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-native": "0.66.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "commitlint": "^15.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "4.3.8",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.3",
    "pod-install": "^0.1.28",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-builder-bob": "^0.18.2",
    "react-test-renderer": "^17.0.2",
    "release-it": "^14.11.6",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": "^16.13 || ^17",
    "react-native": ">=0.63"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn tsc",
      "pre-push": "jest"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint \"**/*.{js,ts,jsx,tsx}\" --fix",
      "prettier --write"
    ]
  }
}
