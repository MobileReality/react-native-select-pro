{
  "name": "react-native-select-pro-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "engines": {
    "node": ">=16.15.1",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev-ios": "turbo run dev-ios",
    "dev-android": "turbo run dev-android",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "e2e": "turbo run test-e2e-ios",
    "tsc": "turbo run tsc",
    "docs": "yarn --cwd website start",
    "postinstall": "husky install",
    "clean": "turbo run clean && rm -Rf node_modules"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@mobile-reality/eslint-config": "^0.3.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^10.1.1",
    "@types/jest": "^28.1.4",
    "@types/react-native": "^0.68.1",
    "@types/react-test-renderer": "^17.0.2",
    "commitlint": "^17.0.1",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-test-renderer": "^17.0.2",
    "reassure": "^0.1.1",
    "ts-jest": "^28.0.5",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'pnpm tsc'",
      "eslint --fix"
    ]
  }
}
