{
  "name": "react-native-select-pro-project",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16.15.1"
  },
  "scripts": {
    "dev-start": "yarn workspace @monorepo/expo start",
    "dev-ios": "yarn workspace @monorepo/expo ios",
    "dev-android": "yarn workspace @monorepo/expo android",
    "e2e-ios-build": "yarn workspace @monorepo/e2e detox-ios-run",
    "e2e-android-build": "yarn workspace @monorepo/e2e detox-android-run",
    "docs": "yarn --cwd website start",
    "tsc": "tsc --noEmit",
    "lint": "eslint ./packages --ext .js,.jsx,.ts,.tsx",
    "test": "jest --forceExit",
    "test-update": "jest -u",
    "build": "bob build",
    "publish": "yarn workspace @mobile-reality/react-native-select-pro release",
    "publish-beta": "yarn workspace @mobile-reality/react-native-select-pro release-beta",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.0",
    "@mobile-reality/eslint-config": "^0.3.0",
    "@release-it/conventional-changelog": "^5.0.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^10.1.1",
    "@types/jest": "^28.1.4",
    "@types/react-native": "^0.68.1",
    "@types/react-test-renderer": "^17.0.2",
    "commitlint": "^17.0.1",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-builder-bob": "^0.18.2",
    "react-test-renderer": "^17.0.2",
    "release-it": "^14.14.3",
    "release-it-yarn-workspaces": "^2.0.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'yarn tsc'",
      "eslint --fix"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
